<table class='table'>
  <tr>
    <th>Size</th>
    <th>Type</th>
    <th>Brand</th>
    <th>Model</th>
    <th>Test Type</th>
    <th>Protocol Name</th>
    <th>Update</th>
    <th>Delete</th>
  </tr>
 
  <% @tests.each do |test| %>
    <tr>
      <td><%= test.size %></td>
      <td><%= test.meter_type %></td>
      <td><%= test.brand %></td>
      <td><%= test.model %></td>
      <td><%= test.test_type %></td>
      <td><%= test.protocol_name %></td>
      <td><%= link_to('Update', edit_test_path(test)) %></td>
      <td><%= link_to('Delete', test_path(test), method: :delete) %></td>
    </tr>
  <% end %>
</table>